# HAAI Environment Configuration
# ================================
# Copy this file to .env and customize the values

# Database Configuration
DB_PASSWORD=haai_secret
DB_NAME=haai_audit
DB_USER=haai_user

# PostgreSQL Admin (pgAdmin)
PGADMIN_PASSWORD=haai_admin

# Redis Commander
REDISCommander_PASSWORD=haai_admin

# Grafana
GRAFANA_PASSWORD=haai_admin

# PyPI API Token (for releases)
# PYPI_API_TOKEN=your-token-here

# GitHub Container Registry (for releases)
# GITHUB_TOKEN=your-github-token-here

# Codecov Token
# CODECOV_TOKEN=your-codecov-token

# Application Settings
HAAI_ENV=development
HAAI_LOG_LEVEL=DEBUG
